format = """
[╭](fg:{{getv "/polar-night/secondary"}})\
[](fg:{{getv "/polar-night/primary"}})\
[ $all](fg:{{getv "/polar-night/text"}} bg:{{getv "/polar-night/primary"}})\
[](fg:{{getv "/polar-night/primary"}})\
